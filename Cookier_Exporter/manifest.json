{
    "manifest_version": 3,
    "name": "Enhanced Cookie Exporter",
    "version": "1.1",
    "description": "Export all cookies with session info and edit cookies",
    "permissions": [
      "cookies",
      "activeTab",
      "downloads",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  